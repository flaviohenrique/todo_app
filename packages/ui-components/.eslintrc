{
  "plugins": [
    "promise"
  ],
  "extends": [
    "standard-react",
    "plugin:promise/recommended"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "legacyDecorators": true,
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "17"
    }
  },
  "rules": {
    "react/prop-types": 0,
    "react/no-unused-prop-types": 0
  }
}
